  {props.users.map((item: PropsUsers, i: number) => (\n                <Row\n                  onEditUser={onEditUser}\n                  deleteItem={deleteItem}\n                  onError={() => notifyError(\"Incorrect user\")}\n                  i={i}\n                  key={i}\n                  item={item}\n                  edit={edit}\n                  editArray={editArray}\n                  open={open}\n                  close={close}\n                />\n              ))}\n            </TableBody>\n          </Table>\n        </TableContainer>\n      </Paper>\n      <ToastContainer />\n    </Wrapper>\n  );\n};\n\nconst Wrapper = styled.div`\n  margin: auto;\n  width: ${WIDTH_CONTENT};\n  @media only screen and ${media.AddUser} {\n    width: 100%;\n    margin-top: 0px;\n  }\n`;\n\nconst mapStateToProps = (state: PropsState) => {\n  return {\n    users: state.users.users,\n  };\n};\n\nconst action = {\n  setUser: actionsUsers.setUser,\n  deleteUser: actionsUsers.deleteUser,\n  editUser: actionsUsers.editUser,\n};\nexport default connect(mapStateToProps, action)(TableComponent);\n","/home/nikolay/Documents/test_task/src/api/consts.tsx",[],"/home/nikolay/Documents/test_task/src/reducer/users.tsx",[],"/home/nikolay/Documents/test_task/src/components/AddUser/index.tsx",["110"],"/home/nikolay/Documents/test_task/src/components/Header/index.tsx",[],["111","112"],"/home/nikolay/Documents/test_task/src/components/Table/Row.tsx",[],"/home/nikolay/Documents/test_task/src/components/AddUser/FormGroup.tsx",["113"],"/home/nikolay/Documents/test_task/src/components/DropMenu/index.tsx",["114","115"],"/home/nikolay/Documents/test_task/src/index.tsx",[],"/home/nikolay/Documents/test_task/src/components/Menu/index.tsx",["116"],"/home/nikolay/Documents/test_task/src/pages/User/index.tsx",["117","118","119","120"],"/home/nikolay/Documents/test_task/src/pages/User/Content.tsx",[],"/home/nikolay/Documents/test_task/src/api/request.tsx",[],"/home/nikolay/Documents/test_task/src/co